package in.co.hsbc.secondpro.arr;

public class StringDemo {
	
	
	public static void main(String[] args) {
		
		String str1 = new String("Java");
		String str2 = new String("HSBC");
		String str3 =  str1;
		System.out.println(str3);
		System.out.println(str2);
		System.out.println(str3);
		
		
		
		
	}

}
