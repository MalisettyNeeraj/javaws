package in.co.hsbc.firstpro.model;

public class Customer {
	
	private String fname;
	private String email;
	
	
	public void setData(String fname,String email) {
		if (fname.length() < 3)
		{
			System.out.println("Invalid fname");
		}
		this.fname = fname;
		this.email = email;
	}
	
	public void printData() {
		System.out.println(fname + " : " + email);
	}
}
