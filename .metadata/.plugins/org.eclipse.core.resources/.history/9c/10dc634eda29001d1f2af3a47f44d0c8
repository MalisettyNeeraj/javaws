package in.co.hsbc.firstpro.model;

public class TV {
	
	private String make;
	private boolean power;
	private int vol=0;
	
	public TV(String make) {
		this.make = make;
	}
	
	public void switchON() {
		power = true;
	}

	public void switchOFF() {
		power = false;
	}
	
	public void volPlus() {
			vol++;
	}
	public void volMinus() {
		vol--;
}
}
